{
    "general": {
        "translationKey": "DASHBOARD_TAB_GENERAL_KEY",
        "displayByDefault": true,
        "maxRecentlyViewedPatients": 10,
        "sections": {
            "patientInformation": {
                "translationKey": "DASHBOARD_TITLE_PATIENT_INFORMATION_KEY",
                "type": "patientInformation",
                "displayOrder": 0,
                "ageLimit": 100,
                "patientAttributes": [],
                "addressFields": [
                ]
            },
            "navigationLinksControl": {
                "translationKey": "DASHBOARD_TITLE_NAVIGATION_LINKS_CONTROL_KEY",
                "type": "navigationLinksControl",
                "displayOrder": 20,
                "showLinks": [
                    "home",
                    "visit",
                    "registration",
                    "visitAttribute"
                ]
            },
            "visits": {
                "translationKey": "DASHBOARD_TITLE_VISITS_KEY",
                "type": "visits",
                "displayOrder": 10,
                "dashboardConfig": {
                    "title": null,
                    "maximumNoOfVisits": 8,
                    "groupByVisits": true
                }
            },
            "formsDisplay": {
                "type": "forms",
                "displayOrder": 30,
                "dashboardConfig": {
                    "title": null,
                    "maximumNoOfVisits": 10
                },
                "formGroup": [],
                "translationKey": "DASHBOARD_TITLE_FORMS_DISPLAY_CONTROL_KEY"
            },
{% for wound_number in wound_numbers %}
            "Wound follow-up - wound {{ wound_number }}": {
                "translationKey": "wound follow-up - wound {{ wound_number }}",
                "hideEmptyDisplayControl": true,
                "displayOrder": 5{{ wound_number }},
                "isObservation": true,
                "dashboardConfig": {
                    "conceptNames": [
                        "forms.follow_up.woundSpecific.painBeforDressing{{ wound_number }}",
                        "forms.follow_up.woundSpecific.size{{ wound_number }}",
                        "forms.follow_up.woundSpecific.tissue{{ wound_number }}",
                        "forms.follow_up.woundSpecific.infection{{ wound_number }}",
                        "forms.follow_up.woundSpecific.moisture{{ wound_number }}",
                        "forms.follow_up.woundSpecific.edges{{ wound_number }}",
                        "forms.follow_up.woundSpecific.periwoundSection{{ wound_number }}",
                        "forms.follow_up.woundSpecific.painAfterDressing{{ wound_number }}"
                    ],
                    "scope": "latest",
                    "showGroupDateTime": false
                },
                "expandedViewConfig": {
                    "conceptNames": [
                        "forms.follow_up.woundSpecific.painBeforDressing{{ wound_number }}",
                        "forms.follow_up.woundSpecific.size{{ wound_number }}",
                        "forms.follow_up.woundSpecific.tissue{{ wound_number }}",
                        "forms.follow_up.woundSpecific.infection{{ wound_number }}",
                        "forms.follow_up.woundSpecific.moisture{{ wound_number }}",
                        "forms.follow_up.woundSpecific.edges{{ wound_number }}",
                        "forms.follow_up.woundSpecific.periwoundSection{{ wound_number }}",
                        "forms.follow_up.woundSpecific.painAfterDressing{{ wound_number }}"
                    ],
                    "showDetailsButton": true,
                    "pivotTable": {
                        "numberOfVisits": "30",
                        "groupBy": "encounters",
                        "obsConcepts": [
                            "forms.follow_up.woundSpecific.painBeforDressing{{ wound_number }}",
                            "forms.follow_up.woundSpecific.size{{ wound_number }}",
                            "forms.follow_up.woundSpecific.tissue{{ wound_number }}",
                            "forms.follow_up.woundSpecific.infection{{ wound_number }}",
                            "forms.follow_up.woundSpecific.moisture{{ wound_number }}",
                            "forms.follow_up.woundSpecific.edges{{ wound_number }}",
                            "forms.follow_up.woundSpecific.periwoundSection{{ wound_number }}",
                            "forms.follow_up.woundSpecific.painAfterDressing{{ wound_number }}"
                        ],
                        "drugConcepts": "",
                        "labConcepts": ""
                    }
                }
            }{{ '' if loop.last else ',' }}
{% endfor %}
        }
    },
    "patientSummary": {
        "translationKey": "DASHBOARD_TAB_PATIENT_SUMMARY_KEY",
        "maxRecentlyViewedPatients": 10,
        "sections": {}
    }
}

